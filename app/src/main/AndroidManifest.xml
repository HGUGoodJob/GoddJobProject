        <activity android:name=".PartPracticePlane4" />
        <activity android:name=".PartPracticeBear1" />
        <activity android:name=".PartPracticeBear2" />
        <activity android:name=".PartPracticeBear3" />
        <activity android:name=".PartPracticeBear4" />
        <activity android:name=".PartPracticeBear5" />
        <activity android:name=".PartPracticeBear6" />
        <activity android:name=".PartPracticeODS1" />
        <activity android:name=".PartPracticeODS2" />
        <activity android:name=".PartPracticeODS3" />
        <activity android:name=".PartPracticeODS4" />
        <activity android:name=".PartPracticeODS5" />
        <activity android:name=".PartPracticeODS6" />
        <activity android:name=".PartPracticeODS7" />
        <activity android:name=".PartPracticeODS8" />
    </application>

</manifest>